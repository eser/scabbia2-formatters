{
    "name": "scabbiafw/scabbia2-formatters",
    "description": "Scabbia2 Formatters Component",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist"
    },
    "keywords": [
        "larukedi",
        "scabbia",
        "scabbia2",
        "scabbiafw",
        "formatters"
    ],
    "homepage": "http://www.scabbiafw.com/",
    "license": [
        "Apache-2.0"
    ],
    "authors": [
        {
            "name": "Scabbia Framework Organization",
            "homepage": "http://scabbiafw.com/"
        },
        {
            "name": "Contributors",
            "homepage": "https://github.com/scabbiafw/scabbia2-formatters/blob/master/contributors.md"
        }
    ],
    "support": {
        "issues": "https://github.com/scabbiafw/scabbia2-formatters/issues",
        "source": "https://github.com/scabbiafw/scabbia2-formatters"
    },
    "require": {
        "php": ">=5.6.0"
    },
    "require-dev": {
        "scabbiafw/scabbia2-tasks": "*",
        "scabbiafw/scabbia2-testing": "*",
        "squizlabs/php_codesniffer": "2.*"
    },
    "suggest": {
    },
    "autoload": {
        "psr-4": {
            "Scabbia\\Formatters\\": [ "src/Formatters/" ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Scabbia\\Tests\\Formatters\\": [ "tests/Formatters/" ]
        }
    }
}
